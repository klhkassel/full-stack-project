      <!-- BMI Calculator Start -->
      <section
        class="common-sec bmi-calculator back-img"
        style="background-image: url('/images/bmi-bg.jpg')"
      >
        <div class="container">
          <div class="row align-items-center">
            <div class="col-lg-6">
              <div class="calculate-bmi">
                <span class="sub-title">Body Mass Index</span>
                <h2 class="h2-title">Calculate Your BMI</h2>
                <div class="calculate-bmi-form">
                  <form id="form" onsubmit="validateForm()">
                    <div class="row">
                      <div class="col-lg-6">
                        <div class="form-box">
                          <input
                            type="text"
                            class="form-input"
                            id="height"
                            placeholder="Height / Cm"
                            required
                            autocomplete="off"
                          />
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="form-box">
                          <input
                            type="text"
                            class="form-input"
                            id="weight"
                            placeholder="Weight / Kg"
                            required
                            autocomplete="off"
                          />
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="form-box">
                          <input
                            type="text"
                            class="form-input"
                            id="age"
                            placeholder="Age"
                            required
                            autocomplete="off"
                          />
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="form-box form-element">
                          <select class="form-input" id="gender">
                            <option value="">Gender</option>
                            <option value="male">Male</option>
                            <option value="female">Female</option>
                            <option value="transgender">Transgender</option>
                          </select>
                          <i class="fas fa-angle-down"></i>
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="form-box form-element">
                          <select class="form-input" id="activity">
                            <option value="">Activity Factor</option>
                            <option value="factor1">Factor 1</option>
                            <option value="factor2">Factor 2</option>
                            <option value="factor3">Factor 3</option>
                          </select>
                          <i class="fas fa-angle-down"></i>
                        </div>
                      </div>
                      <div class="col-lg-6">
                        <div class="form-box">
                          <button
                            type="button"
                            id="calculate_bmi"
                            class="sec-btn"
                          >
                            <span>Calculate Now</span>
                          </button>
                        </div>
                      </div>
                      <div class="col-12">
                        <div class="bmi-result">
                          <p class="h3-title" id="bmi_result"></p>
                          <p id="bmi_count"></p>
                        </div>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
            <div class="col-lg-6">
              <div class="bmi-chart-box">
                <div class="bmi-chart">
                  <div class="bmi-range">
                    <span class="sub-title">BMI Range</span>
                    <ul>
                      <li>Below 18.5</li>
                      <li>18.5 - 24.9</li>
                      <li>25.0 - 29.9</li>
                      <li>30.0 - and Above</li>
                    </ul>
                  </div>
                  <div class="weight-status">
                    <span class="sub-title">Weight Status</span>
                    <ul>
                      <li>Underweight</li>
                      <li>Healthy</li>
                      <li>Overweight</li>
                      <li>Obese</li>
                    </ul>
                  </div>
                  <div class="bmi-fullform">
                    <h6>
                      <span>BMR</span> - Body Metabolic Rate /
                      <span>BMI</span> - Body Mass Index
                    </h6>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
